# Intellicrack Production Environment Configuration
# This file should be populated with actual values in production
# Copy this file to .env and fill in the values

# API Configuration
API_BASE_URL=https://api.intellicrack.com
API_TIMEOUT=60
API_RETRY_ATTEMPTS=3
API_RETRY_DELAY=1000

# License Server Configuration
LICENSE_SERVER_URL=https://license.intellicrack.com
LICENSE_VALIDATION_ENDPOINT=/api/v1/validate
LICENSE_CHECK_INTERVAL=3600

# Update Server Configuration
UPDATE_SERVER_URL=https://updates.intellicrack.com
UPDATE_CHECK_ENDPOINT=/api/v1/check
UPDATE_CHECK_INTERVAL=86400

# Telemetry Configuration
TELEMETRY_ENABLED=false
TELEMETRY_ENDPOINT=https://telemetry.intellicrack.com/api/v1/events
TELEMETRY_BATCH_SIZE=100
TELEMETRY_FLUSH_INTERVAL=300

# Cloud Services Configuration
CLOUD_ANALYSIS_URL=https://cloud.intellicrack.com
CLOUD_ANALYSIS_API_KEY=
CLOUD_STORAGE_BUCKET=intellicrack-analysis
CLOUD_STORAGE_REGION=us-east-1

# Database Configuration
DATABASE_URL=postgresql://user:pass@localhost:5432/intellicrack
DATABASE_POOL_SIZE=10
DATABASE_POOL_TIMEOUT=30

# Cache Configuration
REDIS_URL=redis://localhost:6379/0
CACHE_TTL=3600
CACHE_MAX_SIZE=1000

# Security Configuration
JWT_SECRET_KEY=
ENCRYPTION_KEY=
SSL_CERT_PATH=/etc/ssl/certs/intellicrack.crt
SSL_KEY_PATH=/etc/ssl/private/intellicrack.key

# Network Analysis Configuration
NETWORK_CAPTURE_INTERFACE=eth0
NETWORK_CAPTURE_BUFFER_SIZE=10485760
NETWORK_ANALYSIS_PORT=8443

# Plugin Repository Configuration
PLUGIN_REPO_URL=https://plugins.intellicrack.com
PLUGIN_REPO_API_KEY=
PLUGIN_VERIFY_SIGNATURES=true

# Model Repository Configuration
MODEL_REPO_URL=https://models.intellicrack.com
MODEL_DOWNLOAD_PATH=/var/lib/intellicrack/models
MODEL_CACHE_SIZE=5368709120

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE_PATH=/var/log/intellicrack/app.log
LOG_MAX_SIZE=104857600
LOG_BACKUP_COUNT=10
SENTRY_DSN=

# Performance Configuration
MAX_WORKERS=8
WORKER_TIMEOUT=300
MEMORY_LIMIT=8589934592
GPU_ENABLED=true

# Feature Flags
FEATURE_AI_ANALYSIS=true
FEATURE_CLOUD_SYNC=true
FEATURE_ADVANCED_PATCHING=true
FEATURE_NETWORK_MONITOR=true
FEATURE_PLUGIN_SANDBOX=true

# External Service Keys
VIRUSTOTAL_API_KEY=
HYBRID_ANALYSIS_API_KEY=
SHODAN_API_KEY=
CENSYS_API_KEY=

# Webhook Configuration
WEBHOOK_ENABLED=false
WEBHOOK_URL=
WEBHOOK_SECRET=

# Rate Limiting
RATE_LIMIT_ENABLED=true
RATE_LIMIT_REQUESTS_PER_MINUTE=60
RATE_LIMIT_BURST_SIZE=100

# Monitoring
PROMETHEUS_ENABLED=false
PROMETHEUS_PORT=9090
HEALTH_CHECK_PORT=8080

# Exploitation Settings
EXPLOIT_LHOST=0.0.0.0
EXPLOIT_LPORT=4444

# Ollama Settings
OLLAMA_API_BASE=http://localhost:11434

# License Server Settings
LICENSE_SERVER_BIND_IP=0.0.0.0
LICENSE_SERVER_PORTS=27000,27001,1111,1234,1337,8080,8888
LICENSE_SERVER_DNS_REDIRECT=true
LICENSE_SERVER_SSL_INTERCEPT=true
LICENSE_SERVER_RECORD_TRAFFIC=true
LICENSE_SERVER_AUTO_RESPOND=true
LICENSE_SERVER_RESPONSE_DELAY=0.1
LICENSE_SERVER_DEFAULT_PROTOCOL=FlexLM

# License Settings
LICENSE_USER_ID=
LICENSE_FEATURES=FULL_SUITE

# OpenAI Settings
OPENAI_API_KEY=
OPENAI_API_ENDPOINT=https://api.openai.com/v1

# HTTP Proxy Settings
HTTP_PROXY=
HTTPS_PROXY=

# C2 Domain Settings
C2_DNS_DOMAIN=example.com

# Sandbox Detection Settings
SANDBOX_SUSPICIOUS_COMPUTERS=sandbox,malware,virus,test,vmware,virtualbox,qemu,analysis

# API Settings
API_KEY=