# Intellicrack Environment Configuration Template
# Copy this file to .env.local and add your actual API keys
# NEVER commit .env.local or any file with real API keys to version control

# Priority: .env.local > .env.production > .env > .env.example

# LLM API Keys (Required for AI features)
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GOOGLE_API_KEY=your_google_api_key_here

# Additional LLM Providers (Optional)
COHERE_API_KEY=
HUGGINGFACE_API_TOKEN=
GROQ_API_KEY=
TOGETHER_API_KEY=
XAI_API_KEY=
OPENROUTER_API_KEY=

# Security Analysis Services (Optional)
VIRUSTOTAL_API_KEY=
HYBRID_ANALYSIS_API_KEY=
SHODAN_API_KEY=
CENSYS_API_KEY=

# Cloud Service Keys (Optional)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AZURE_API_KEY=

# Application Secrets (Auto-generated if not provided)
JWT_SECRET_KEY=
ENCRYPTION_KEY=

# Ollama Settings (Optional - for local models)
OLLAMA_API_BASE=http://localhost:11434

# Feature Flags (Optional)
ENABLE_CLOUD_ANALYSIS=false
ENABLE_TELEMETRY=false
DEBUG_MODE=false

# License Server Settings (Advanced)
LICENSE_SERVER_BIND_IP=0.0.0.0
LICENSE_SERVER_AUTO_RESPOND=true

# Note: intellicrack_config.json handles most configuration
# This file is primarily for API keys and secrets
