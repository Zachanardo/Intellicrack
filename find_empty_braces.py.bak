#!/usr/bin/env python3
"""Find empty braces in f-string"""

content = open('intellicrack/ui/dialogs/plugin_manager_dialog.py', 'r', encoding='utf-8').read()
lines = content.split('\n')
f_string_start = 764
f_string_end = 1359

print(f'Checking lines {f_string_start+1} to {f_string_end+1}')
for i in range(f_string_start, f_string_end+1):
    if '{}' in lines[i]:
        print(f'Line {i+1}: {lines[i].strip()}')