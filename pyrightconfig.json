{
  "include": [
    "intellicrack/"
  ],
  "exclude": [
    "**/__pycache__",
    "**/node_modules",
    "**/.venv*",
    "**/venv*",
    "**/env*",
    "**/.env*",
    "**/virtualenv*",
    "**/pythonenv*",
    "**/pyenv*",
    "**/*_venv",
    "**/*_env",
    "**/.conda",
    "**/conda-env",
    "**/build",
    "**/dist",
    "**/.git",
    "**/.pytest_cache",
    "**/.mypy_cache",
    "**/.ruff_cache",
    "**/htmlcov",
    "**/site-packages",
    "**/data",
    "**/logs",
    "**/cache",
    "**/.cache",
    "**/temp",
    "**/tmp",
    "**/*.egg-info",
    "**/plugin_cache",
    "**/.analysis_cache",
    "**/.intellicrack_cache",
    "**/model_cache",
    "**/ml_training_cache",
    "docs/_build",
    "docs/build",
    ".venv_windows",
    ".venv_wsl",
    ".zen_venv",
    "temp_venv"
  ],
  "typeCheckingMode": "basic",
  "useLibraryCodeForTypes": false,
  "reportMissingImports": "warning",
  "pythonVersion": "3.10",
  "venvPath": ".",
  "venv": ".venv_windows"
}