{
  "include": [
    "intellicrack/"
  ],
  "exclude": [
    "**/__pycache__",
    "**/node_modules",
    "**/.venv*",
    "**/venv*",
    "**/env*",
    "**/.env*",
    "**/virtualenv*",
    "**/pythonenv*",
    "**/pyenv*",
    "**/*_venv",
    "**/*_env",
    "**/.conda",
    "**/conda-env",
    "**/build",
    "**/dist",
    "**/.git",
    "**/.pytest_cache",
    "**/.mypy_cache",
    "**/.ruff_cache",
    "**/htmlcov",
    "**/site-packages",
    "**/data",
    "**/logs",
    "**/cache",
    "**/.cache",
    "**/temp",
    "**/tmp",
    "**/*.egg-info",
    "**/plugin_cache",
    "**/.analysis_cache",
    "**/.intellicrack_cache",
    "**/model_cache",
    "**/ml_training_cache",
    "docs/_build",
    "docs/build",
    ".venv_wsl",
    ".zen_venv",
    "temp_venv",
    "mamba_env/",
    "mamba_env/**",
    "**/mamba_env/**",
    "C:/Intellicrack/mamba_env/**",
    "**/.serena",
    "**/tests",
    "**/test_*",
    "**/tensorflow/**",
    "**/torch/**",
    "**/site-packages/**",
    "dev/**",
    "tools/**"
  ],
  "typeCheckingMode": "off",
  "useLibraryCodeForTypes": true,
  "reportMissingImports": "none",
  "pythonVersion": "3.12",
  "pythonPath": "C:/Intellicrack/.pixi/envs/default/python.exe",
  "analyzeUnannotatedFunctions": true,
  "enableTypeIgnoreComments": true,
  "verboseOutput": false,
  "stubPath": "typings",
  "ignore": [
    "mamba_env",
    "dev",
    "tools"
  ]
}
