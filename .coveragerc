[run]
source = intellicrack
omit =
    */tests/*
    */migrations/*
    */__pycache__/*
    */venv/*
    */mamba_env/*
branch = True

[report]
fail_under = 95
precision = 2
show_missing = True
skip_covered = False
sort = Cover

[html]
directory = coverage_html_report
title = Intellicrack Coverage Report

[xml]
output = coverage.xml
