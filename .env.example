# Intellicrack Environment Variables
# Copy this file to .env and configure as needed

# C2 Server Configuration
# ----------------------

# HTTPS Protocol Configuration
C2_HTTPS_ENABLED=false
C2_HTTPS_HOST=0.0.0.0
C2_HTTPS_PORT=443

# DNS Protocol Configuration
C2_DNS_ENABLED=false
C2_DNS_DOMAIN=your-domain.com
C2_DNS_HOST=0.0.0.0
C2_DNS_PORT=53

# TCP Protocol Configuration (Default)
C2_TCP_ENABLED=true
C2_TCP_HOST=0.0.0.0
C2_TCP_PORT=4444

# API Keys (Optional)
# -------------------
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
OPENROUTER_API_KEY=
GOOGLE_API_KEY=

# License Configuration
# --------------------
# LICENSE_USER_ID=auto  # Uses system username by default
# LICENSE_FEATURES=FULL_SUITE,PREMIUM,ENTERPRISE
# LICENSE_DAYS_VALID=365

# Advanced Configuration
# ---------------------
